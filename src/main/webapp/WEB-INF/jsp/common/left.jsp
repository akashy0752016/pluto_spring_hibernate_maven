<div class="col-md-3 main-left">
				<h5 class="head">Railway User</h3>
				<h6 class="head_sub">PMS Help Desk: 18004200234</h6>
				<form id="railway_user">
					<div class="form-group row">
					    <label for="staticEmail" class="col-sm-4 col-form-label">User Id</label>
					    <div class="col-sm-8">
					      <input type="text" class="form-control form-control-sm" name="userId" id="userId">
					    </div>
					</div>
					<div class="form-group row">
					    <label for="inputPassword" class="col-sm-4 col-form-label">Password</label>
					    <div class="col-sm-8">
					      <input type="password" class="form-control form-control-sm" name="password" id="password">
					    </div>
					</div>
					<div class="form-group row">
						<div class="col-sm-12">
							<select class="form-control form-control-sm" name="shift" id="shift">
							  <option value="">Select Shift</option>
							  <option value="GENERAL SHIFT">GENERAL SHIFT</option>
							</select>
						</div>
					</div>
					<div class="form-group row">
					    <label for="inputPassword" class="col-sm-4 col-form-label">Enter Station Code</label>
					    <div class="col-sm-8">
					      <input type="text" class="form-control form-control-sm" id="stationCode" name="stationCode">
					    </div>
					</div>
					<div class="form-group row">
					    <label for="inputPassword" class="col-sm-4 col-form-label">Private Cash(Rs.)</label>
					    <div class="col-sm-8">
					      <input type="number" value="0" class="form-control form-control-sm" id="privateCash" name="privateCash">
					    </div>
					</div>
					<div class="form-group row">
					    <label for="inputPassword" class="col-sm-4 col-form-label">Running Stationary No.</label>
					    <div class="col-sm-8">
					      <input type="number" value="0" class="form-control form-control-sm" id="runningStationaryNo" name="runningStationaryNo">
					    </div>
					</div>
					<div class="form-group row">
						<div class="col-sm-12">
							<select class="form-control form-control-sm" name="bookingType" id="bookingType">
							  <option value="">Select Booking</option>
							  <option value="PWBK-PARCEL">PWBK-PARCEL Booking</option>
							</select>
						</div>
					</div>
					<div class="form-group row">
				    <label for="inputPassword" class="col-sm-4 col-form-label">POS Term.ID*</label>
				    <div class="col-sm-8">
				      <input type="text" class="form-control form-control-sm" id="posTerm" name="posTerm">
				    </div>
				</div>
				<div class="form-group row">
				    <label for="inputPassword" class="col-sm-4 col-form-label">Counter No.</label>
				    <div class="col-sm-8">
				      <input type="number" value="1" class="form-control form-control-sm" id="counterNumber" name="counterNumber">
				    </div>
				</div>
				<div class="form-action">
					<button type="submit" class="btn">Submit</button>
				</div>
				<div class="form-group row">
				<div class="col-sm-12 change_pass">
					<a href="#">
				    	<label for="inputPassword" class="col-form-label">Change Password</label>
				    </a>
				</div>
			</div>
		</form>
